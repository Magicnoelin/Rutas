{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "warn",
    "prefer-const": "warn",
    "no-var": "warn"
  },
  "globals": {
    "document": "readonly",
    "window": "readonly",
    "console": "readonly",
    "setTimeout": "readonly",
    "clearTimeout": "readonly"
  }
}
